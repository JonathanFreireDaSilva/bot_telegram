{
  "compilerOptions": {
    "experimentalDecorators": true,
    "outDir": "./Dist/Server",
    "allowJs": true,
    "target": "es6",
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true
  },
  "include": ["Server/**/*"],
  "inlineSourceMap": true,
  "exclude": ["node_modules", "**/*.test.ts"]
}
